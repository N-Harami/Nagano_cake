<%= form_with model: @item, url: admin_item_path do |f| %>

<h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_area :description %>
  <h4>単価</h4>
  <%= f.number_field :tax_excluded_price %>
  <h4>商品画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>販売ステータス</h4>
  <%= f.radio_button :is_sale_status, "true" %>販売中
  <%= f.radio_button :is_sale_status, "false" %>販売停止中
  <%= f.submit '保存' %>

  <% end %>