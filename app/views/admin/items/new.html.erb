<h1>新規登録</h1>
<%= form_with model: @item, url: admin_items_path do |f| %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_area :description %>
  <h4>単価</h4>
  <%= f.number_field :tax_excluded_price %>
  <%= f.select :is_sale_status, [['販売中',true],['売切れ',false]] %>

  <h4>商品画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <%= f.submit '登録' %>
  <% end %>